//speed detector
const prompt=require("prompt-sync")();
const speed= parseInt(prompt("Recorded speed"))
function carspeed(speed){
    if(speed<70){
        console.log("Ok")
    }
    else if(speed>70){
        const points=((speed - 70) / 5);{
            console.log('demerit points. &(points)');
        }
    }
    else if (points >=12){
        console.log("License suspended")
    }
};